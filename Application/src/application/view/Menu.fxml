<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="700.0" prefWidth="750.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.controllers.Menu">
   <top>
      <HBox prefHeight="42.0" prefWidth="750.0" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <ScrollPane hbarPolicy="NEVER" prefWidth="750.0" BorderPane.alignment="TOP_CENTER">
         <content>
            <VBox alignment="CENTER" prefHeight="600.0" prefWidth="750.0" spacing="15.0">
               <children>
                  <Label text="VARpedia" underline="true">
                     <font>
                        <Font size="91.0" />
                     </font>
                  </Label>
                  <Button fx:id="_create" mnemonicParsing="false" onAction="#openSearch" prefHeight="60.0" prefWidth="400.0" text="Create">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Button>
                  <Button fx:id="_videos" mnemonicParsing="false" onAction="#videoList" prefHeight="60.0" prefWidth="400.0" text="Videos">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Button>
                  <Button fx:id="_help" mnemonicParsing="false" onAction="#openHelp" prefHeight="50.0" prefWidth="60.0" text="?">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
